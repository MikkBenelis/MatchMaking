{
	"Logging": {
		"Console": {
			"LogLevel": {
				"Microsoft.AspNetCore": "Warning",
				"Microsoft.Hosting.Lifetime": "Warning",
				"MatchMakingService.Services.PayloadServices.MatchUserProducerService": "Information",
				"MatchMakingService.Services.PayloadServices.MatchResultConsumerService": "Information"
			},
			"FormatterName": "simple",
			"FormatterOptions": {
				"SingleLine": true,
				"IncludeScopes": true,
				"TimestampFormat": "[yyyy.MM.dd HH:mm:ss] "
			}
		}
	},
	"ConnectionStrings": {
		"Kafka": "host.docker.internal:9092",
		"Redis": "host.docker.internal:6379"
	},
	"Kestrel": {
		"Endpoints": {
			"Http": {
				"Url": "http://0.0.0.0:800"
			},
			"Https": {
				"Url": "https://0.0.0.0:4430",
				"Certificate": {
					"Path": "MatchMakingServiceAPI.pfx",
					"Password": "default"
				}
			}
		}
	},
	"AllowedHosts": "*",
	"AllowSwagger": true,
	"ForceHTTPSRedirect": false,
	"MatchMaking": {
		"KafkaTopics": {
			"Request": "matchmaking.request",
			"Complete": "matchmaking.complete"
		},
		"RedisLogging": {
			"IsActive": true,
			"LifetimeHours": 24
		},
		"RateLimiter": {
			"MillisecondsLimit": 100,
			"RequestsCountLimit": 1,
			"RequestsQueueLimit": 0,
			"IsActiveGlobally": true,
			"IsActiveForMatchSearch": true,
			"IsActiveForMatchStatus": false
		}
	}
}