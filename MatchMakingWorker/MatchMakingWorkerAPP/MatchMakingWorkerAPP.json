{
	"Logging": {
		"Console": {
			"LogLevel": {
				"Microsoft.Hosting.Lifetime": "Warning",
				"MatchMakingWorker.Services.PayloadServices.MatchUserConsumerService": "Information",
				"MatchMakingWorker.Services.PayloadServices.MatchResultProducerService": "Information"
			},
			"FormatterName": "simple",
			"FormatterOptions": {
				"SingleLine": true,
				"IncludeScopes": true,
				"TimestampFormat": "[yyyy.MM.dd HH:mm:ss] "
			}
		}
	},
	"ConnectionStrings": {
		"Kafka": "host.docker.internal:9092",
		"Redis": "host.docker.internal:6379"
	},
	"MatchMaking": {
		"KafkaTopics": {
			"Request": "matchmaking.request",
			"Complete": "matchmaking.complete"
		},
		"RedisLogging": {
			"IsActive": true,
			"LifetimeHours": 24
		},
		"GroupSize": 3
	}
}