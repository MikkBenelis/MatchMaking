{
	"Logging": {
		"Console": {
			"LogLevel": {
				"Microsoft.Hosting.Lifetime": "Warning",
				"MatchMakingWorker.Services.MatchUserConsumerService": "Information",
				"MatchMakingWorker.Services.MatchResultProducerService": "Information"
			},
			"FormatterName": "simple",
			"FormatterOptions": {
				"SingleLine": true,
				"IncludeScopes": true,
				"TimestampFormat": "yyyy.MM.dd HH:mm:ss "
			}
		}
	},
	"ConnectionStrings": {
		"Redis": "host.docker.internal:6379",
		"Kafka": "host.docker.internal:9092"
	},
	"MatchMaking": {
		"KafkaTopics": {
			"Request": "matchmaking.request",
			"Complete": "matchmaking.complete"
		},
		"GroupSize": 3
	}
}